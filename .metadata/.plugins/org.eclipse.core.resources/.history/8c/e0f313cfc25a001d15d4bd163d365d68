package Hello;


class helloworld
{	
	static int a=10;
		   int b=20;
	
	static void method1() {
		System.out.println("method 1");
	}
	void method2() {
		System.out.println("method 2");
	}
	public static void main(String[] args) {
		System.out.println(a);
		System.out.println(new helloworld().b);
		method1();
		new helloworld().method2();
	}
}
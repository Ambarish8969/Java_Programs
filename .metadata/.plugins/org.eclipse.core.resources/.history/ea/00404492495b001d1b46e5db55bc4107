package Hello;

class Assign2{
	int a=100;
	int b=200;
	int result=a+b;
	void add() {
		System.out.println(result);
	}
}

public class Assign1 {
	int c=10;
	int d=30;
	public static void main(String[] args) {
		System.out.println(new Assign1().c);
		System.out.println(new Assign2().a);
		System.out.println(new Assign2().b);
		System.out.println(new Assign1().d);
		
	}
	
}

